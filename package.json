    "@angular/language-service": "~9.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@fluffy-spoon/substitute": "^1.117.0",
    "@ngneat/spectator": "^5.3.1",
    "@types/faker": "^4.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
