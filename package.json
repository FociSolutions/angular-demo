{
  "name": "angular-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:docs": "compodoc -p ./tsconfig.json --theme Laravel -w -s --includes .",
    "build": "ng build",
    "test": "ng test",
    "test:docs": "compodoc -d ./public -p ./tsconfig.json --theme Laravel --coverageTest --coverageTestThresholdFail false  --includes .",
    "build:ci": "ng build --prod --sourceMap=false --base-href /angular-demo/",
    "build:docs": "compodoc -d ./public -p ./tsconfig.json --theme Laravel  --includes .",
    "test:ci": "ng test --watch=false --code-coverage --source-map true",
    "lint": "ng lint",
    "e2e": "ng e2e --dev-server-target",
    "e2e:ci": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@rxweb/reactive-form-validators": "^1.9.9-beta4",
    "ngx-logger": "^4.1.2",
    "rxjs": "~6.5.4",
